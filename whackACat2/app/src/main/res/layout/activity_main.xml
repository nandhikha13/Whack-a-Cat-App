        app:srcCompat="@drawable/kitty" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="211dp"
        android:layout_height="68dp"
        android:layout_marginStart="172dp"
        android:layout_marginTop="656dp"
        android:fontFamily="@font/allerta"
